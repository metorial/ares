{
  "name": "ares-service",
  "module": "index.ts",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "start": "bun run server:start",
    "dev": "bun --watch ./src/server.ts",
    "server:build": "bun build ./src/server.ts --outdir ./dist --target bun --sourcemap=external",
    "server:start": "bun ./dist/index.js",
    "frontend:build": "bun run frontend:build:auth && bun run frontend:build:admin",
    "frontend:build:auth": "cd frontend/auth && vite build",
    "frontend:build:admin": "cd frontend/admin && vite build",
    "frontend:dev": "cd frontend/auth && vite",
    "infra:up": "docker compose -p ares -f ./docker-compose.dev.yml --profile infra up -d --pull always",
    "infra:down": "docker compose -p ares -f ./docker-compose.dev.yml --profile infra down",
    "infra:logs": "docker compose -p ares -f ./docker-compose.dev.yml --profile infra logs -f",
    "db:generate": "bun prisma generate",
    "db:push:dev": "sh -c 'set -a; . .env; set +a; bun prisma db push --accept-data-loss'",
    "db:push:test": "sh -c 'set -a; . .env.test; set +a; bun prisma db push --accept-data-loss'",
    "db:push": "bun run db:push:dev && bun run db:push:test",
    "test": "bunx vitest run --passWithNoTests",
    "test:watch": "bunx vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@lowerdeck/testing-tools": "^1.0.1",
    "@types/bun": "latest",
    "@types/node": "^25.0.3",
    "@vercel/ncc": "^0.38.1",
    "@vitejs/plugin-react": "^5.1.4",
    "vitest": "4.0.17"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.957.0",
    "@aws-sdk/client-iam": "^3.957.0",
    "@aws-sdk/client-lambda": "^3.958.0",
    "@looped/hooks": "^0.1.3",
    "@lowerdeck/api-mux": "^1.0.1",
    "@lowerdeck/cache": "^1.0.0",
    "@lowerdeck/canonicalize": "^1.0.4",
    "@lowerdeck/cron": "^1.0.4",
    "@lowerdeck/encryption": "^1.0.6",
    "@lowerdeck/env": "^1.0.4",
    "@lowerdeck/error": "^1.0.8",
    "@lowerdeck/event": "^1.0.0",
    "@lowerdeck/forwarded-for": "^1.0.4",
    "@lowerdeck/hash": "^1.0.4",
    "@lowerdeck/hono": "^1.0.7",
    "@lowerdeck/id": "^1.0.5",
    "@lowerdeck/ip-info": "^1.0.4",
    "@lowerdeck/once": "^1.0.4",
    "@lowerdeck/pagination": "^1.0.4",
    "@lowerdeck/queue": "^1.0.4",
    "@lowerdeck/rpc-client": "^1.0.2",
    "@lowerdeck/rpc-server": "^1.0.4",
    "@lowerdeck/sentry": "^1.0.2",
    "@lowerdeck/service": "^1.0.3",
    "@lowerdeck/slugify": "^1.0.4",
    "@lowerdeck/snowflake": "^1.0.2",
    "@lowerdeck/tokens": "^1.0.5",
    "@lowerdeck/validation": "^1.0.4",
    "@marsidev/react-turnstile": "^1.4.2",
    "@metorial-io/data-hooks": "^1.0.1",
    "@metorial-io/layout": "^1.0.1",
    "@metorial-io/pages": "^1.0.0",
    "@metorial-io/ui": "^1.0.0",
    "@metorial-services/forge-client": "^1.0.3",
    "@metorial-services/relay-client": "^1.0.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/themes": "^3.3.0",
    "@sentry/bun": "^10.38.0",
    "@types/ua-parser-js": "^0.7.39",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
